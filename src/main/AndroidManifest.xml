<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sj.custom">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.sj.custom_view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.sj.custom_view.practice.layout.TagActivity"/>
        <activity android:name="com.sj.custom_view.practice.layout.MyViewPagerAct"/>
        <activity android:name="com.sj.custom_view.practice.scable.ScableActivity"/>
        <activity android:name="com.sj.custom_view.practice.touch.MultiTouchActivity"/>
        <activity android:name="com.sj.custom_view.simulation.jk.JKSelectActivity"/>
        <activity android:name="com.sj.custom_view.simulation.jk.JKRefreshActivity"/>
    </application>

</manifest>